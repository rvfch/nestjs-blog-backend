{
  "name": "backend",
  "version": "2.1.0-DEV",
  "description": "Blog Engine",
  "author": "Roman Filatov",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/apps/backend/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/backend/test/jest-e2e.json",
    "migrate-db": "npx sequelize-cli db:migrate",
    "seed-db": "npx sequelize-cli db:seed:all"
  },
  "dependencies": {
    "@apollo/server": "^4.8.1",
    "@nestjs/apollo": "^12.0.7",
    "@nestjs/axios": "^3.0.0",
    "@nestjs/bull": "^10.0.1",
    "@nestjs/cache-manager": "^2.0.1",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/graphql": "^12.0.8",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/microservices": "^10.0.5",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/sequelize": "^10.0.0",
    "@nestjs/serve-static": "^4.0.0",
    "@nestjs/swagger": "^7.1.1",
    "@nestjs/terminus": "^10.0.1",
    "@nestjs/throttler": "^4.2.1",
    "@types/handlebars": "^4.1.0",
    "apollo-server-express": "^3.12.0",
    "axios": "^1.4.0",
    "bull": "^4.10.4",
    "cache-manager": "^5.2.3",
    "cache-manager-redis-yet": "^4.1.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cookie-parser": "^1.4.6",
    "dayjs": "^1.11.9",
    "eslint-plugin-spellcheck": "^0.0.20",
    "graphql": "^16.7.1",
    "graphql-subscriptions": "^2.0.0",
    "graphql-tools": "^9.0.0",
    "graphql-ws": "^5.14.0",
    "handlebars": "^4.7.7",
    "helmet": "^7.0.0",
    "http-proxy-middleware": "^2.0.6",
    "ioredis": "^5.3.2",
    "joi": "^17.9.2",
    "multer": "^1.4.5-lts.1",
    "nestjs-throttler-storage-redis": "^0.3.3",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.11.1",
    "pg-hstore": "^2.3.4",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "sequelize": "^6.32.1",
    "sequelize-typescript": "^2.1.5",
    "slugify": "^1.6.6",
    "subscriptions-transport-ws": "^0.11.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.1.3",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.3.1",
    "@types/passport-jwt": "^3.0.8",
    "@types/passport-local": "^1.0.35",
    "@types/sequelize": "^4.28.15",
    "@types/sinon": "^10.0.16",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "argon2": "^0.30.3",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-unused-imports": "^3.0.0",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "sequelize-cli": "^6.6.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@app/common(|/.*)$": "<rootDir>/libs/common/src/$1"
    }
  }
}
